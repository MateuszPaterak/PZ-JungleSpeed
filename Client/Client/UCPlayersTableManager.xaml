<UserControl x:Class="Client.PlayersTableManager"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Client"
             mc:Ignorable="d" Height="500" Width="500">
    <Grid x:Name="GridPlayer" Margin="0,0,0,0" Height="500" Width="500" HorizontalAlignment="Left" VerticalAlignment="Top">
        <Grid.Background>
            <ImageBrush x:Name="ImgBackground" ImageSource="Pictures/wood500_2.png"/>
        </Grid.Background>
        <Image x:Name="ImgTotem" Source="/Pictures/totem.png" HorizontalAlignment="Center" Height="150" Margin="0,0,0,0" VerticalAlignment="Center" Width="Auto" />
    </Grid>
</UserControl>
